[core]
  editor = atom --wait
  autocrlf = input
  safecrlf = true
  quotepath = false
  excludesFile = ~/.gitignore_global
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[push]
  default = current
